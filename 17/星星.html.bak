 <body>
	<script>
		function random(n,m){
			return Math.floor(Math.random() * (m - n + 1) + n)
		}

		setInterval(function(){
			var str = `<img src=./xingxing.png 
			style=width:${random(20,200)}px;position:absolute;top:${random(0,800)}px;left:${random(0,1700)}px>`

			document.body.innerHTML += str
			document.body.style.backgroundColor = "#" + Math.random().toString(16).substr(2,6)

		},100)

	</script>
 </body>
